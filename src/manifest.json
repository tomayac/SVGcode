{
  "name": "SVGcode",
  "short_name": "SVGcode",
  "start_url": "https://svgco.de/",
  "id": "https://svgco.de/",
  "description": "SVGcode is a Progressive Web App that lets you convert raster images like JPG, PNG, GIF, WebP, AVIF, etc. to vector graphics in SVG format.",
  "screenshots": [
    {
      "src": "https://svgco.de/screenshots/desktop.png",
      "sizes": "2828x1988",
      "type": "image/png"
    }
  ],
  "scope": "https://svgco.de/",
  "display": "standalone",
  "dir": "auto",
  "display_override": ["window-controls-overlay"],
  "orientation": "portrait",
  "theme_color": "#131313",
  "background_color": "#131313",
  "user_preferences": {
    "color_scheme": {
      "dark": {
        "theme_color": "#131313",
        "background_color": "#131313"
      },
      "light": {
        "theme_color": "#fffe",
        "background_color": "#fffe"
      }
    }
  },
  "icons": [
    {
      "src": "https://svgco.de/favicon.png",
      "type": "image/png",
      "sizes": "1024x1024",
      "purpose": "maskable"
    },
    {
      "src": "https://svgco.de/favicon.svg",
      "type": "image/svg+xml",
      "sizes": "150x150",
      "purpose": "any"
    },
    {
      "src": "https://svgco.de/favicon-bw.svg",
      "type": "image/svg+xml",
      "sizes": "150x150",
      "purpose": "monochrome"
    }
  ],
  "file_handlers": [
    {
      "action": "https://svgco.de/",
      "accept": {
        "image/*": [
          ".jpg",
          ".jpeg",
          ".webp",
          ".png",
          ".avif",
          ".gif",
          ".svg",
          ".bmp"
        ]
      }
    }
  ],
  "share_target": {
    "action": "https://svgco.de/share-target/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "image",
          "accept": ["image/jpeg", "image/png", "image/webp", "image/gif"]
        }
      ]
    }
  },
  "iarc_rating_id": "39d78758-59f2-429e-92ab-ad11d61feafe",
  "categories": ["photo", "productivity", "utilities", "devtools"],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=de.svgco.twa",
      "id": "de.svgco.twa"
    },
    {
      "platform": "windows",
      "url": "https://www.microsoft.com/en-us/p/svgcode/9plhxdgsw1rj",
      "id": "9PLHXDGSW1RJ"
    }
  ],
  "prefer_related_applications": false
}
